{
  "name": "@erikwithuhk/html-to-jsx",
  "version": "0.0.0",
  "description": "Converts HTML to JSX for use with React",
  "publishConfig": {
    "access": "public"
  },
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib",
    "clean": "rimraf lib",
    "lint": "eslint ./src",
    "prepare": "npm run build",
    "release": "npm run np",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikwithuhk/html-to-jsx.git"
  },
  "keywords": [
    "react",
    "jsx",
    "htmltojsx",
    "html",
    "react-component",
    "parser"
  ],
  "author": "Daniel Lo Nigro <daniel@dan.cx> (http://dan.cx/)",
  "contributors": [
    "erikwithuhk"
  ],
  "maintainers": [
    "erikwithuhk"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/reactjs/react-magic/issues"
  },
  "homepage": "https://github.com/reactjs/react-magic",
  "dependencies": {
    "jsdom-no-contextify": "~3.1.0",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "yargs": "~4.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "eslint": ">=4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^22.4.3",
    "np": "^2.20.1",
    "prettier": "^1.11.1",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2"
  }
}
