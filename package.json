{
  "name": "@erikwithuhk/html-to-jsx",
  "version": "1.0.6",
  "description": "Converts HTML to JSX for use with React",
  "main": "lib/index.js",
  "files": [
    "/lib/**/*.js"
  ],
  "scripts": {
    "build": "npm run clean && babel src --out-dir lib --copy-files",
    "build:watch": "npm run build -- --watch",
    "clean": "rimraf lib",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikwithuhk/html-to-jsx.git"
  },
  "keywords": [
    "react",
    "jsx",
    "htmltojsx",
    "html",
    "react-component",
    "parser"
  ],
  "author": "Daniel Lo Nigro <daniel@dan.cx> (http://dan.cx/)",
  "contributors": [
    "erikwithuhk",
    "gimlids",
    "kohlmannj"
  ],
  "maintainers": [
    "erikwithuhk"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/reactjs/react-magic/issues"
  },
  "homepage": "https://github.com/reactjs/react-magic",
  "dependencies": {
    "@kohlmannj/css-to-object": "^1.2.1",
    "js-string-escape": "^1.0.1",
    "jsdom-no-contextify": "~3.1.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "yargs": "~17.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.23.5",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "^18.2.0",
    "regenerator-runtime": "^0.14.0",
    "rimraf": "^5.0.5"
  },
  "packageManager": "yarn@4.0.2"
}
